{
  "name": "shiny-listbox",
  "version": "1.1.0",
  "description": "create listbox components in shiny",
  "author": "@dcruvolo",
  "status": "active",
  "scripts": {
    "shiny": "Rscript -e \"shiny::runApp(launch.browser = FALSE, port = 8000)\"",
    "dev": "parcel src/index.scss src/listbox.scss -d www/css/ --no-source-maps --no-cache --no-minify",
    "build": "parcel build src/index.scss src/listbox.scss -d www/css/ --no-source-maps --no-cache",
    "clean": "rm -rf www/css/*",
    "start": "concurrently \"npm run shiny\" \"npm run dev\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidruvolo51/shinyAppTutorials.git"
  },
  "keywords": [
    "r",
    "shiny",
    "tutorials"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidruvolo51/shinyAppTutorials/issues"
  },
  "homepage": "https://github.com/davidruvolo51/shinyAppTutorials",
  "devDependencies": {
    "@babel/cli": "^7.12.16",
    "@babel/core": "^7.12.16",
    "@babel/preset-env": "^7.12.16",
    "autoprefixer": "^10.2.4",
    "babel-preset-minify": "^0.5.1",
    "concurrently": "^5.3.0",
    "parcel": "^1.12.4",
    "postcss-modules": "^4.0.0",
    "sass": "^1.32.7"
  }
}
